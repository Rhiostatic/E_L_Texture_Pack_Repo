{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "minecraft:squid",
      "materials": { "default": "squid" },
      "textures": {
        "default": "textures/entity/squid",
        "default2": "textures/entity/squid2",
        "default3": "textures/entity/squid3"
      },
      "geometry": {
        "default": "geometry.squid"
      },
      "animations": {
        "move": "animation.squid.move",
        "squid_rotate": "animation.squid.rotate"
      },
      "scripts": {
        "animate": [
          "move",
          "squid_rotate"
        ],
        "initialize": [
          "variable.xvar = math.random(0,999);"
        ]
      },
      "render_controllers": [ "controller.render.squid" ],
      "spawn_egg": {
        "texture": "spawn_egg",
        "texture_index": 15
      }
    }
  }
}
