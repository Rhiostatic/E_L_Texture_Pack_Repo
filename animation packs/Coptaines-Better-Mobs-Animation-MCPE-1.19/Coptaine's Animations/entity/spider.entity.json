{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "minecraft:spider",
      "min_engine_version": "1.8.0",
      "materials": {
        "default": "spider",
        "invisible": "spider_invisible"
      },
      "geometry": {
        "default": "geometry.spider"
      },
      "textures": {
        "default": "textures/entity/spider/spider"
      },
      "scripts": {
        "scale": "q.life_time < 0.2 ? q.life_time * 5 : 1",
        "pre_animation": [
          "variable.tcos0 = (Math.cos(query.modified_distance_moved * 38.17) * query.modified_move_speed) * 57.3;",
          "variable.tcos1 = (Math.cos(query.modified_distance_moved * 76.34) * query.modified_move_speed) * 57.3;"
        ],
        "animate": [
          "root",
          "move",
          { "look_at_target": "query.is_alive" },
          { "dead": "!query.is_alive" }
        ],
        "initialize": [
          "v.roll = math.random_integer(1, 5);"
        ]
      },
      "animations": {
        "move": "controller.animation.spider.move",
        "root": "controller.animation.spider.root",
        "look_at_target": "animation.spider.look_at_target",
        "base_pose": "animation.spider.base_pose",
        "climb": "animation.spider.climb",
        "attack": "animation.spider.attack",
        "idle1": "animation.spider.idle1",
        "idle2": "animation.spider.idle2",
        "idle3": "animation.spider.idle3",
        "dead": "animation.spider.dead",
        "default_leg_pose": "animation.spider.default_leg_pose",
        "walk": "animation.spider.walk"
      },
      "render_controllers": [ "controller.render.spider" ],
      "spawn_egg": {
        "texture": "spawn_egg",
        "texture_index": 11
      }
    }
  }
}