      "animation_controllers": [
        { "move": "controller.animation.endermite.move" }
      ],
      "render_controllers": [ "controller.render.endermite" ],
      "spawn_egg": {
        "texture": "spawn_egg",
        "texture_index": 25
      }
    }
  }
}
