{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "minecraft:sheep",
      "min_engine_version": "1.8.0",
      "materials": { "default": "sheep" },
      "textures": {
        "default": "textures/entity/sheep/sheep"
      },
      "geometry": {
        "default": "geometry.sheep",
        "sheared": "geometry.sheep.sheared"
      },
      "scripts": {
        "scale": "q.life_time < 0.2 ? q.life_time * 5 : 1",
        "pre_animation": [
          "variable.tcos0 = (Math.cos(query.modified_distance_moved * 38.17) * query.modified_move_speed) * 57.3;",
          "variable.tcos1 = (Math.cos(query.modified_distance_moved * 76.34) * query.modified_move_speed) * 57.3;"
        ],
        "animate": [
          "root",
          { "look_at_target": "query.is_alive" },
          { "sheep_big_head": "query.is_baby" },
          { "dead": "!query.is_alive" }
        ],
        "initialize": [
          "v.roll = math.random_integer(1, 5);"
        ]
      },
      "animations": {
        "root": "controller.animation.sheep.root",
        "sheep_baby_big_head": "controller.animation.sheep.baby_big_head",
        "look_at_target": "animation.sheep.look_at_target",
        "sheep_big_head": "animation.sheep.baby_transform",
        "base_pose": "animation.sheep.base_pose",
        "base_pose_sus": "animation.sheep.base_pose_sus",
        "idle1": "animation.sheep.idle1",
        "idle2": "animation.sheep.idle2",
        "idle3": "animation.sheep.idle3",
        "move_upper_body": "animation.sheep.move_upper_body",
        "move_lower_body": "animation.sheep.move_lower_body",
        "panic": "animation.sheep.panic",
        "grazing": "animation.sheep.grazing",
        "dead": "animation.custom_death",
        "dead1": "animation.sheep.dead1",
        "dead2": "animation.sheep.dead2",
        "dead3": "animation.sheep.dead1",
        "dead4": "animation.sheep.dead2"
      },
      "render_controllers": [ "controller.render.sheep" ],
      "spawn_egg": {
        "texture": "spawn_egg",
        "texture_index": 3
      }
    }
  }
}
