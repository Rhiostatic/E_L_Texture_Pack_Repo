{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.stray": {
      "rebuild_animation_matrices": true,
      "geometry": "q.is_alive ? Geometry.default : Geometry.dead",
      "materials": [ { "*": "Material.default" } ],
      "textures": [ "Texture.default" ],
      "is_hurt_color": {
        "r": "q.is_alive ? 1 : 0",
        "g": "q.is_alive ? 0 : 0",
        "b": "q.is_alive ? 0 : 0",
        "a": "q.is_alive ? 0.6 : 0"
      },
      "on_fire_color": {
        "r": "q.is_alive ? 1 : 0",
        "g": "q.is_alive ? 0 : 0",
        "b": "q.is_alive ? 0 : 0",
        "a": "q.is_alive ? 0.6 : 0"
      }
    }
  }
}