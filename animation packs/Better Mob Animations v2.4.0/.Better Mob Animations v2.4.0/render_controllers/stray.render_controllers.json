{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.stray": {
      "geometry": "geometry.default",
      "materials": [
        {
          "*": "material.default"
        }
      ],
      "textures": ["texture.default"]
    },
    "controller.render.stray_clothes": {
      "geometry": "geometry.overlay",
      "materials": [
        {
          "*": "material.overlay"
        }
      ],
      "textures": ["texture.overlay"],
      "is_hurt_color": {
        "r": 1.0,
        "g": 1.0,
        "b": 1.0,
        "a": "q.is_alive ? 0.2 : 0"
      }
    }
  }
}
